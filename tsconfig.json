{
    "compilerOptions": {     
      "module": "es6",
      "target": "ES5",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true
    },
    "include": ["www/src/**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }